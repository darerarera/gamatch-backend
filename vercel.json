{
  "version": 2,
  "builds": [
    { "src": "wsgi.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/wsgi.py" }
  ],
  "env": {
    "SECRET_KEY": "@secret_key",
    "MAIL_USERNAME": "@mail_username",
    "MAIL_PASSWORD": "@mail_password",
    "DATABASE_URL": "@database_url"
  }
}
